{
  "name": "soqqle",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "rm -rf DerivedData && node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "bundle-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --sourcemap-output android/app/src/main/assets/android-release.bundle.map",
    "bundle-ios": "node node_modules/react-native/local-cli/cli.js bundle --platform ios --dev false --entry-file index.js --bundle-output ios/ios-release.bundle --sourcemap-output bugsnag/ios-release.bundle.map --assets-dest ios",
    "debug-apk": "node node_modules/react-native/local-cli/cli.js bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --sourcemap-output bugsnag/android-release.bundle.map --assets-dest android/app/src/main/res/ && cd android && ./gradlew assembleDebug",
    "release-apk": "cd android && ./gradlew assembleRelease",
    "list-codepush-releases-android": "appcenter codepush deployment list -a infrasoqqle/Soqqle-1 --displayKeys",
    "list-codepush-releases-ios": "appcenter codepush deployment list -a infrasoqqle/Soqqle --displayKeys",
    "iosDeployementKeyList": "code-push deployment ls infrasoqqle/Soqqle -k",
    "code-pushAndroid": "appcenter codepush release-react -a infrasoqqle/Soqqle-1 -d Staging --mandatory",
    "code-pushIos": "appcenter codepush release-react -a infrasoqqle/Soqqle -d Staging --mandatory",
    "androidBundleBuild": "react-native bundle --platform android --dev false --entry-file index.js  --bundle-output android/app/src/main/assets/index.android.bundle  --assets-dest android/app/src/main/res/",
    "android-build": "watchman watch-del-all && cd android && ./gradlew --stop && ./gradlew clean && ./gradlew assembleRelease"
  },
